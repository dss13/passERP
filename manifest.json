{
  "manifest_version": 2,

  "name": "PassERP",
  "description": "Store your ERP credentials and auto-login",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/",
    "storage"
  ],

  "content_scripts" : [
    {
      "matches":["https://erp.iitkgp.ac.in/*"],
      "js":["popup.js","script.js"]
    }
  ]
}
